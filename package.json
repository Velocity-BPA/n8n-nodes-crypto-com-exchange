{
  "name": "n8n-nodes-crypto-com-exchange",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Crypto.com Exchange providing spot trading, derivatives, margin trading, wallet management, and market data operations with HMAC-SHA256 authentication.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-crypto-com-exchange.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-crypto-com-exchange#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-crypto-com-exchange/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "crypto.com",
    "cryptocurrency",
    "exchange",
    "trading",
    "spot-trading",
    "derivatives",
    "margin-trading",
    "wallet",
    "market-data",
    "automation",
    "workflow"
  ],
  "main": "dist/nodes/CryptoComExchange/CryptoComExchange.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/CryptoComExchangeApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/CryptoComExchange/CryptoComExchange.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist/",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.22.0",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
